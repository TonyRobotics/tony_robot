<?xml version="1.0"?>
<launch>
    <param name="use_sim_time" value="false" />

    <node pkg="tf" type="static_transform_publisher" name="broadcaster_laser_map"   
     args="0.09 0 0 3.1416 0 0 /base_link /laser_link 100" />
    
    <!-- motor trd -->
    <include file="$(find trd_diff_controller)/launch/trd_diff_controller.launch">
        <arg name="serialport_name" value="/dev/motor_trd" />
        <arg name="baudrate" value="38400" />
        <arg name="left_coef" value="-1.0" />
        <arg name="right_coef" value="-1.0" />
        <arg name="linear_coef" value="105" />
        <arg name="angular_coef" value="13.2" />
        <arg name="encoder_ticks_per_rev" value="1320" />
        <arg name="base_width" value="0.25" />
        <arg name="wheel_diameter" value="0.08" />
    </include>

    <!-- ls01c -->
    <include file="$(find ls01c)/launch/ls01c.launch">
    </include>

    <!-- astra RGB-D -->
    <!--   
    <include file="$(find tr10_launch)/launch/rgbd_astra.launch">
    </include>
    -->    

    <!-- kinect RGB-D -->
    <!--
    <include file="$(find tr10_launch)/launch/rgbd_kinect.launch">
    </include>    
    -->

</launch>

