<?xml version="1.0"?>
<launch>
    <param name="use_sim_time" value="false" />
    <!-- move_base -->
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true"> 
        <rosparam file="$(find tr10_launch)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find tr10_launch)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find tr10_launch)/config/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find tr10_launch)/config/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find tr10_launch)/config/base_local_planner_params.yaml" command="load" />
    </node>
</launch>

