<?xml version="1.0"?>
<launch>
    <param name="use_sim_time" value="false" />

    <node pkg="tf" type="static_transform_publisher" name="broadcaster_base_laser"
          args="0.225 0 0 3.1416 0 0 /base_link /laser_link 100" />
    
    <!-- motor trd -->
    <include file="$(find trd_diff_controller)/launch/trd_diff_controller.launch">
        <arg name="serialport_name" value="/dev/motor_trd" />
        <arg name="baudrate" value="38400" />
        <arg name="left_coef" value="-1.0" />
        <arg name="right_coef" value="1.0" />
        <arg name="linear_coef" value="82.0" />
        <arg name="angular_coef" value="14.64" />
        <arg name="encoder_ticks_per_rev" value="1600" />
        <arg name="base_width" value="0.39" />
        <arg name="wheel_diameter" value="0.125" />
    </include>

    <!-- laser leishen -->
    <include file="$(find lslaser)/launch/leishen.launch">
    </include>
    
</launch>

