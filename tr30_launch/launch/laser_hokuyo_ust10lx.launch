<?xml version="1.0"?>
<launch>
    <arg name="ip_address" default="192.168.0.10" />
    <arg name="angle_min" default="-0.8" />
    <arg name="angle_max" default="0.8" />
    <node pkg="urg_node" type="urg_node" name="urg_node">
        <param name="ip_address" value="$(arg in_address)"/>
        <param name="angle_min" value="$(arg angle_min)"/>
        <param name="angle_max" value="$(arg angle_max)"/>
    </node>
</launch>

